{
  "AWSEBDockerrunVersion": "2",
  "volumes": [
    {
      "name": "app",
      "host": {
        "sourcePath": "/var/app/current/app"
      }
    },
    {
      "name": "db",
      "host": {
        "sourcePath": "/var/app/current/db"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "app",
      "image": "jenniferwg/timebox:app",
      "environment": [],
      "essential": true,
      "memory": 128,
      "portMappings": [{
        "hostPort": 3030,
        "containerPort": 3030
      }],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-app",
          "containerPath": "/var/log"
        }
      ]
    },
    {
      "name": "db",
      "image": "jenniferwg/timebox:db",
      "essential": true,
      "memory": 128,
      "portMappings": [{
        "hostPort": 3031,
        "containerPort": 3031
      }],
      "links": [
        "app"
      ],
      "mountPoints": [] 
    }
  ]
}